{
  "name": "@notainc/typed-api-spec",
  "version": "0.4.0",
  "scripts": {
    "install:optional-peer-dependencies": "npm install --no-save zod valibot@0 express@4 @types/express@4 fastify fastify-type-provider-zod@2",
    "build": "tsup",
    "watch:build": "tsup --watch",
    "format": "prettier --write .",
    "watch:type-check": "npx tsc --noEmit --watch",
    "test": "run-p test:*",
    "test:unit": "vitest",
    "test:lint": "eslint .",
    "test:format": "prettier --check .",
    "test:type-check": "tsc --noEmit"
  },
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.9.15",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-strict-dependencies": "^1.3.13",
    "npm-run-all2": "^7.0.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.0.0",
    "supertest": "^7.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@types/express": "^4",
    "@types/express-serve-static-core": "^4",
    "@valibot/to-json-schema": "^1.0.0-beta.4",
    "express": "^4",
    "fastify": "^4 || ^5.0.0",
    "fastify-type-provider-zod": "^2",
    "typescript": "^5.3",
    "valibot": "1.0.0-beta.11",
    "zod": "^3.23.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "valibot": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "@types/express": {
      "optional": true
    },
    "@types/express-serve-static-core": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "fastify-type-provider-zod": {
      "optional": true
    }
  },
  "files": [
    "./dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "require": "./dist/core/index.js",
      "import": "./dist/core/index.mjs"
    },
    "./express": {
      "types": "./dist/express/index.d.ts",
      "require": "./dist/express/index.js",
      "import": "./dist/express/index.mjs"
    },
    "./fastify": {
      "types": "./dist/fastify/index.d.ts",
      "require": "./dist/fastify/index.js",
      "import": "./dist/fastify/index.mjs"
    },
    "./fetch": {
      "types": "./dist/fetch/index.d.ts",
      "require": "./dist/fetch/index.js",
      "import": "./dist/fetch/index.mjs"
    },
    "./json": {
      "types": "./dist/json/index.d.ts",
      "require": "./dist/json/index.js",
      "import": "./dist/json/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.3",
    "@standard-schema/spec": "^1.0.0",
    "json-schema": "^0.4.0",
    "json-schema-walker": "^2.0.0",
    "path-to-regexp": "^8.2.0"
  },
  "optionalDependencies": {
    "zod-openapi": "^4.2.2"
  }
}
